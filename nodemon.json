{
    "ignore": [
        "./.git",
        ".dist",

        ".eslintignore",
        ".eslintrc.json",
        ".gitignore",
        ".prettierignore",
        ".prettierrc.cjs",
        ".prettierrc.js",
        ".swcrc",
        "tsconfig.json",
        "tsconfig.prod.json",

        "./src/test",
        "*.test.ts",
        "*.test.mts",
        "*.test.cts",
        "*.test.js",
        "*.test.mjs",
        "*.test.cjs"
    ],
    "execMap": {
        "js": "node",
        "ts": "babel-node --config-file './babel.config.cjs.js' --extensions '.ts,.cts,.mts,.js,.cjs,.mjs' {{filename}}"
    },
    "ext": "js,ts,json"
}
